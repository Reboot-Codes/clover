{
  "nixEnvSelector.nixFile": "${workspaceFolder}/flake.nix",
  "cSpell.words": [
    "evtbuzz"
  ],
  "deno.enablePaths": [
    "./toolbox/spanner",
    "./docs"
  ],
  "rust-analyzer.check.workspace": false,
  "rust-analyzer.numThreads": 4,
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer",
    "editor.formatOnSave": true
  }
}
